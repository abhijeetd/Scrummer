<script type="text/ng-template" id="teamMemberEditor">
        <div class="modal-header">
            Team member editor
        </div>
        <div class="modal-body">
            <div class="row">
                <div class="col-md-6 ">
                    <sub>FIRST NAME</sub>
                    <input type="text" ng-model="vm.newObject.firstname" class="form-control" />
                    <sub>LAST NAME</sub>
                    <input type="text" ng-model="vm.newObject.lastname" class="form-control" />
                    <sub>ROLE</sub>
                    <input type="text" ng-model="vm.newObject.role" class="form-control" />
                    <sub>LOCATION</sub>
                    <input type="text" ng-model="vm.newObject.location" class="form-control" />
                    <sub>EMAIL</sub>
                    <input type="text" ng-model="vm.newObject.email" class="form-control" />
                    <sub>PROJECT CODE</sub>
                    <input type="text" ng-model="vm.newObject.projectCode" class="form-control" />
                    <br />
                </div>
            </div>
        </div>
        <div class=" modal-footer text-right">
            <button class="btn btn-primary" ng-click="vm.updateTeammember()">Done</button>
            <button class="btn btn-default" ng-click="vm.close()">Cancel</button>
        </div>
</script>

